{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ]
    }
  }
}