{
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "@components": "./src/components",
                    "@navigation": "./src/navigation",
                    "@assets": "./src/assets",
                    "@store": "./src/store",
                    "@screens": "./src/screens",
                    "@hooks": "./src/hooks"
                }
            }
        ]
    ]
}